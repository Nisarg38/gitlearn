# GitLearn Environment Configuration
# Copy this file to .env and fill in the values

# GitHub Configuration (required for GitHub Action)
GITHUB_TOKEN=your_github_token_here

# OpenAI/Anthropic API Key (for AI-powered extraction)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# GitLearn Configuration
GITLEARN_BATCH_SIZE=10
GITLEARN_MAX_ITERATIONS=50
GITLEARN_MODEL=gpt-4

# Optional: Debug mode
DEBUG=false
